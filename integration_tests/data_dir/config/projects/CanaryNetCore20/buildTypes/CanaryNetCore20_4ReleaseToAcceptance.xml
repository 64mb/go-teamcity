<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="d91d3a72-c707-4e49-bb6c-a1edc623c64a" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>4. Release To Acceptance</name>
  <description>Deploys and validates a release to Acceptance environment</description>
  <settings>
    <options>
      <option name="buildNumberPattern" value="%env.RELEASE_VERSION%" />
    </options>
    <parameters>
      <param name="env.ARTIFACTS_ROOT_URL" value="%dep.CanaryNetCore20_2BuildRelease.env.ARTIFACTS_ROOT_URL%" />
      <param name="env.RELEASE_ENVIRONMENT" value="Acceptance" />
      <param name="env.RELEASE_VERSION" value="%dep.CanaryNetCore20_2BuildRelease.env.RELEASE_VERSION%" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_224" name="Invoke build.ps1 with 'release' target" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_noprofile" value="true" />
          <param name="jetbrains_powershell_scriptArguments" value="-Target release -Verbosity %verbosity%" />
          <param name="jetbrains_powershell_script_file" value="build.ps1" />
          <param name="jetbrains_powershell_script_mode" value="FILE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="CanaryNetCore20_CanaryDotnetcore20" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_205" type="buildDependencyTrigger">
        <parameters>
          <param name="afterSuccessfulBuildOnly" value="true" />
          <param name="branchFilter" value="+:&lt;default&gt;" />
          <param name="dependsOn" value="CanaryNetCore20_3ReleaseToTesting" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <dependencies>
      <depend-on sourceBuildTypeId="CanaryNetCore20_3ReleaseToTesting">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

