<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="c3947e89-671b-4416-91e4-0aa13948e7e0" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2017.2/project-config.xsd">
  <name>2. Build Release</name>
  <description>Build new release with "buildrelease" target</description>
  <settings>
    <options>
      <option name="buildNumberPattern" value="2.0.%build.counter%" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_220" name="Invoke build.ps1 with 'buildrelease' target" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_noprofile" value="true" />
          <param name="jetbrains_powershell_scriptArguments" value="-Target buildrelease -Verbosity %verbosity%" />
          <param name="jetbrains_powershell_script_file" value="build.ps1" />
          <param name="jetbrains_powershell_script_mode" value="FILE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="CanaryNetCore20_CanaryDotnetcore20" />
    </vcs-settings>
    <requirements>
      <does-not-equal id="RQ_93" name="teamcity.agent.environment" value="shared" />
    </requirements>
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:pull/*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
          <param name="triggerRules" value="+:*" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
</build-type>

