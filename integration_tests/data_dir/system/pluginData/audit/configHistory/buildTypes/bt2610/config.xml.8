<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="f91f31fd-7317-46f3-bdf1-1a4ce8072ffb" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2017.2/project-config.xsd">
  <name>1. Pull Request</name>
  <description>Inspection Build with "pullrequest" target</description>
  <settings>
    <options>
      <option name="buildNumberPattern" value="2.0.%build.counter%" />
      <option name="maximumNumberOfBuilds" value="10" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_221" name="Invoke build.ps1 with 'pullrequest' target" type="jetbrains_powershell">
